data{
    N <- length(y)
}

model{
    for(i in 1:N){
        y[i] ~ dpois(theta[i])
        log(theta[i]) <- b0 + z[i]*(b1[1]*normal[i] + b1[2]*wbc[i]) + (1-z[i])*b1[2]*wbc[i]
        z[i] ~ dbin(p, 1)
    }
    p ~ dbeta(1, 1) ## proportion of bins with a non-zero normal tissue contribution
    b0 ~ dnorm(0,0.1)
    b1 ~ ddirich(c(1,1))
}
