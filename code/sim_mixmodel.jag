data{
    N <- length(y)
}

model{
    for(i in 1:N){
        y[i] ~ dpois(theta[i])
        #log(theta[i]) <- b1[1]*normal[i] + b1[2]*wbc[i] + b0

        log(theta[i]) <- b0 + (z*(b1[1]*normal[i] + b1[2]*wbc[i])) + ((1-z)*(b1[2]*wbc[i]))
    }
    z ~ dbin(0.5, 1)
    b0 ~ dnorm(0,0.1)
    b1 ~ ddirich(c(1,1))
}
